  <%= simple_form_for [@list, @bookmark] do |f| %>
    <%= f.input :movie_id, collection: Movie.all.map { |movie| [movie.title, movie.id] } %>
    <%= f.input :comment %>
    <div class="d-flex justify-content-between">
      <%= f.button :submit, "Add Movie", class: "btn btn-primary" %>
    </div>
  <% end %>
